{
  "name": "restfuncs-server",
  "version": "0.10.0",
  "description": "Serve a REST API for your plain functions and seamlessly RPC-call them from the client (browser). Works with express or standalone.",
  "keywords": ["rpc","rest"],
  "author": "Boris Gingold <bogeee@bogitech.de>",
  "repository": {
    "type": "git",
    "url": "https://github.com/bogeeee/restfuncs.git",
    "directory": "server"
  },
  "license": "MIT",
  "main": "index.js",
  "exports": {
    ".": {
      "browser": "./dist/mjs/index.js",
      "default": "./index.js"
    },
    "./Util": {
      "browser": "./dist/mjs/Util.js",
      "default": "./Util.js"
    }
  },
  "scripts": {
    "clean": "tsc --build --clean && rm -rf dist LICENSE readme.md",
    "build": "tsc && tsc -module ES6 --outDir dist/mjs"
  },
  "dependencies": {
    "restfuncs-common": "^2.0",
    "express": "^4.17.13",
    "express-session": "^1.17.3",
    "underscore": "^1.13.3",
    "typescript-rtti": "^0.9.6",
    "reflect-metadata": "^0.1.13",
    "@brillout/json-serializer": "^0.5.3",
    "escape-html": "^1.0.3",
    "busboy": "^1.6.0",
    "engine.io": "^6.5.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.3",
    "@types/underscore": "^1.11.4",
    "@types/escape-html": "^1.0.2",
    "@types/busboy": "^1.5.0",
    "ts-node": "^10.9.1",
    "typescript": "4.9",
    "nodemon": "^2.0.15"
  }
}

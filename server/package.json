{
  "name": "restfuncs-server",
  "version": "0.8.0",
  "description": "Serve a REST API for your plain functions and seamlessly RPC-call them from the client (browser). For use with express or standalone.",
  "main": "index.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "browser": "./dist/mjs/index.js",
      "default": "./index.js"
    }
  },
  "scripts": {
    "clean": "tsc --build --clean && rm -rf dist LICENSE readme.md",
    "build": "tsc && tsc -module ES6 --outDir dist/mjs"
  },
  "dependencies": {
    "express": "^4.17.13",
    "express-session": "^1.17.3",
    "underscore": "^1.13.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.3",
    "@types/underscore": "^1.11.4",
    "ts-node": "^10.6.0",
    "typescript": "^4.8.4",
    "nodemon": "^2.0.15"
  },
  "license": "MIT"
}

{
  "name": "restfuncs-server",
  "version": "3.3.0",
  "description": "Serve a REST API for your plain functions and seamlessly RPC-call them from the client (browser).",
  "keywords": ["rpc","rest"],
  "author": "Boris Gingold <bogeee@bogitech.de>",
  "repository": {
    "type": "git",
    "url": "https://github.com/bogeeee/restfuncs.git",
    "directory": "server"
  },
  "license": "MIT",
  "main": "index.js",
  "exports": {
    ".": {
      "import": "./index_esm.mjs",
      "require": "./dist/commonjs/index.js"
    },
    "./__the_following_entries_are_for_development_for_jest_with_commonjs__": {},
    "./__Util_is_also_used_by_tests-crossSiteSecurity__": {},
    "./Util": {
      "default": "./dist/commonjs/Util.js"
    },
    "./Server": {
      "require": "./dist/commonjs/Server.js"
    },
    "./ServerSession": {
      "require": "./dist/commonjs/ServerSession.js"
    },
    "./CommunicationError": {
      "require": "./dist/commonjs/CommunicationError.js"
    }
  },
  "scripts": {
    "dev:fastbuild": "tsc --build",
    "clean": "tsc --build --clean && rimraf dist LICENSE readme.md",
    "build": "tsc --build --force"
  },
  "dependencies": {
    "restfuncs-common": "^3.2",
    "express": "^4.17.13",
    "@types/express": "^4.17.13",
    "express-session": "^1.17.3",
    "@types/express-session": "^1.17.3",
    "underscore": "^1.13.3",
    "@types/underscore": "^1.11.4",
    "typescript-rtti": "^0.9.6",
    "reflect-metadata": "^0.1.13",
    "@brillout/json-serializer": "^0.5.3",
    "escape-html": "=1.0.3",
    "@types/escape-html": "=1.0.4",
    "busboy": "^1.6.0",
    "@types/busboy": "^1.5.0",
    "engine.io": "^6.5.3",
    "tweetnacl": "^1.0.3",
    "tweetnacl-util": "^0.15.1",
    "typia": "^5.0.0",
    "clone": "^2.1.2",
    "@types/clone": "^2.1.4"
  },
  "devDependencies": {
    "restfuncs-transformer": "^1.1.0",
    "rimraf": "=5.0.5"
  },
  "optionalDependencies": {
    "restfuncs-transformer": "^1.1.0"
  }
}

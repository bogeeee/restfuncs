{
  "name": "restfuncs-example-server",
  "version": "0.8.0",
  "description": "",
  "type": "module",
  "scripts": {
    "dev": "nodemon --exec cross-env NODE_ENV=development tsx server.ts",
    "clean": "ttsc --build --clean && rm -rf ../client/dist",
    "build": "ttsc --build --force && npm run build:client",
    "build:client": "cd ../client && vite build",
    "start": "ts-node server.js"
  },
  "dependencies": {
    "express": "^4.17.13",
    "@types/express": "^4.17.13",
    "vite": "^2.9.15",
    "restfuncs-server": "^0.8.0"
  },
  "devDependencies": {
    "tsx": "^3.12.1",
    "nodemon": "^2.0.15",
    "cross-env": "^7.0.3"
  }
}

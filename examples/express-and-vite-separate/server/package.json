{
  "name": "restfuncs-example-server",
  "version": "0.9.0",
  "description": "",
  "type": "module",
  "scripts": {
    "dev": "nodemon -e ts --exec \"clear && ttsc --build && cross-env NODE_ENV=development ts-node server.js\"",
    "dev:faster_no_security": "npm run clean && nodemon --exec cross-env NODE_ENV=development tsx server.ts",
    "clean": "ttsc --build --clean && rm -rf ../client/dist",
    "build": "ttsc --build --force && npm run build:client",
    "build:client": "cd ../client && vite build",
    "start": "ts-node server.js"
  },
  "dependencies": {
    "express": "^4.17.13",
    "@types/express": "^4.17.13",
    "vite": "^4.4.11",
    "restfuncs-server": "^0.x"
  },
  "devDependencies": {
    "tsx": "^3.12.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.6",
    "ttypescript": "^1.5.15",
    "nodemon": "^2.0.15",
    "cross-env": "^7.0.3"
  }
}

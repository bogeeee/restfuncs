{
  "name": "restfuncsexampleserver",
  "version": "0.8.0",
  "description": "",
  "type": "module",
  "scripts": {
    "dev": "nodemon -e ts --exec npm run dev_compileAndRunOnce",
    "dev_compileAndRunOnce": "clear && tsc --incremental && ts-node index",
    "build": "tsc && npm run build:client",
    "build:client": "cd ../client && vite build",
    "run": "cross-env NODE_ENV=production ts-node index.js"
  },
  "dependencies": {
    "express": "^4.17.13",
    "vite": "^2.9.15",
    "@restfuncs/server": "^0.8.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "ts-node": "^10.6.0",
    "typescript": "^4.8.4",
    "nodemon": "^2.0.15",
    "cross-env": "^7.0.3"
  }
}

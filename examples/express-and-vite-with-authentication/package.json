{
  "name": "restfuncs-example-authentication",
  "version": "2.0.0",
  "description": "",
  "type": "module",
  "main": "./server",
  "scripts": {
    "dev": "npm run clean && nodemon --exec cross-env NODE_ENV=development DISABLESECURITY=true tsx server.ts",
    "dev:with_validation": "nodemon -e ts --ignore client --exec \"clear && tspc --build && cross-env NODE_ENV=development ts-node server.js\"",
    "clean": "tspc --build --clean && rm -rf client/dist",
    "build": "tspc --build --force && cd client && vite build",
    "start": "ts-node server.js"
  },
  "dependencies": {
    "express": "^4.17.13",
    "@types/express": "^4.17.13",
    "vite": "4.1.4",
    "restfuncs-server": "^2.0.0",
    "restfuncs-client": "^2.0.0",
    "helmet": "^7.1.0"
  },
  "devDependencies": {
    "tsx": "^3.12.1",
    "ts-node": "^10.9.1",
    "ts-patch": "^3.0.2",
    "restfuncs-transformer": "^0.9.6",
    "nodemon": "^2.0.15",
    "cross-env": "^7.0.3"
  }
}

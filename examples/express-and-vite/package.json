{
  "name": "restfuncs-example",
  "version": "2.0.0",
  "description": "",
  "type": "module",
  "main": "./server",
  "scripts": {
    "dev": "npm run clean && nodemon --exec cross-env NODE_ENV=development tsx server.ts",
    "clean": "tspc --build --clean && rimraf client/dist",
    "build": "tspc --build --force && cd client && vite build",
    "start": "cross-env NODE_ENV=production node --enable-source-maps server.js"
  },
  "dependencies": {
    "express": "^4.17.13",
    "@types/express": "^4.17.13",
    "vite": "4.1.4",
    "restfuncs-server": "^3.0.0",
    "restfuncs-client": "^2.0.0",
    "helmet": "^7.1.0"
  },
  "devDependencies": {
    "tsx": "^4.7.0",
    "ts-patch": "^3.0.2",
    "restfuncs-transformer": "^1.0.0",
    "nodemon": "^2.0.15",
    "cross-env": "^7.0.3",
    "rimraf": "=5.0.5"
  }
}

{
  "name": "restfuncse-example",
  "version": "0.8.0",
  "description": "",
  "type": "module",
  "main": "./server",
  "scripts": {
    "dev": "nodemon -e ts --exec npm run dev_compileAndRunOnce",
    "dev_compileAndRunOnce": "clear && ttsc --build && ts-node server.js",
    "clean": "tsc --build --clean && rm -rf tsconfig.tsbuildinfo dist",
    "build": "ttsc --build --force && vite build",
    "start": "cross-env NODE_ENV=production ts-node server.js"
  },
  "dependencies": {
    "express": "^4.17.13",
    "vite": "^2.9.15",
    "restfuncs-server": "^0.8.0",
    "restfuncs-client": "^0.8.0"
  },
  "devDependencies": {
    "ts-node": "^10.6.0",
    "typescript": "4.6",
    "ttypescript": "^1.5.15",
    "nodemon": "^2.0.15",
    "cross-env": "^7.0.3"
  }
}

{
  "name": "restfuncs-session-playground",
  "version": "0.9.0",
  "description": "",
  "type": "module",
  "main": "./server",
  "scripts": {
    "prepare": "npm run --prefix ../.. build",
    "dev": "nodemon -e ts --ignore client --exec \"clear && tspc --build && cross-env NODE_ENV=development node --enable-source-maps server.js\"",
    "clean": "tspc --build --clean && rm -rf client/dist"
  },
  "dependencies": {
    "express": "^4.17.13",
    "@types/express": "^4.17.13",
    "vite": "4.1.4",
    "restfuncs-server": "^2.0.0",
    "restfuncs-client": "^2.0.0"
  },
  "devDependencies": {
    "tsx": "^4.7.0",
    "ts-patch": "^3.0.2",
    "restfuncs-transformer": "^0.9.6",
    "nodemon": "^2.0.15",
    "cross-env": "^7.0.3"
  }
}

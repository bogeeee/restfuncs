{
  "name": "restfuncse-cross-site-security-tests",
  "version": "1.0",
  "description": "",
  "type": "module",
  "main": "./server",
  "scripts": {
    "dev": "tspc --build && cross-env NODE_ENV=development ts-node server.js",
    "dev:withServerBuild": "npm run --prefix ../../server build && npm run dev",

    "clean": "tspc --build --clean && rm -rf client/dist",
    "build": "tspc --build --force && cd client && vite build",
    "start": "ts-node server.js"
  },
  "dependencies": {
    "express": "^4.17.13",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.7",
    "vite": "4.1.4",
    "restfuncs-server": "0.x",
    "restfuncs-client": "^1.0.0",
    "helmet": "^7.1.0"
  },
  "devDependencies": {
    "tsx": "^3.12.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.6",
    "ts-patch": "^3.0.2",
    "restfuncs-transformer-tsc": "^0.9.6",
    "nodemon": "^2.0.15",
    "cross-env": "^7.0.3"
  }
}
